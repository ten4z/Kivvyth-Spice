<Sc_Manager>:
	Sc_Image:
		id: sc_img
		name: "sc_img"
<Sc_Image>:
	AnchorLayout:
		anchor_x: "center"
		anchor_y: "center" 
		MDBoxLayout
			orientation: "horizontal"
			padding: 5
			spacing: 5
			MDGridLayout:
				cols: 2
				rows: 8
				spacing: 10
				margin: 60
				padding: "10sp"
				CustomMDCard:
					line_color: (0.2, 0.2, 0.8, 1.0)
					style: "filled"
					md_bg_color: "#34ffff"										 
					size_hint: 1, None
					size: "200dp", "100dp"
					on_release: app.root.get_screen("sc_img").show_popup('2802527')
					MDRelativeLayout:
						md_bg_color: 239/255, 239/255, 239/255, 1
						name:"entrance"
						FitImage:			
							source:"images/2802527.jpg"
							opacity: 0.75
						MDIconButton:
							icon: "link-variant"
							pos_hint: {"top": 1, "right": 1}
							theme_icon_color: "Custom"
							icon_color: app.theme_cls.primary_color
							on_release: root.open_link("https://www.pexels.com/pt-br/foto/especiarias-para-cozinhar-variadas-2802527/")
						MDLabel:
							id: label
